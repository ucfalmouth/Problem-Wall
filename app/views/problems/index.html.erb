<%= form_for @problem.new, url: {action: "create"} do |f| %>
  <%= f.text_field :name %>
  <%= f.text_field :url %>
  <%= f.text_area :description %>
  <%= f.submit "Create" %>
<% end %>
<h1>Problems Wall</h1>
<% @problem.each do |problem| %>
	<div><%= link_to problem.name, problem %></div>
<% end %>
